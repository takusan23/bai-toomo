{
	"width": 640,
	"height": 320,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"title": {
			"type": "image",
			"path": "image/title.png",
			"width": 640,
			"height": 320
		},
		"end": {
			"type": "image",
			"path": "image/end.png",
			"width": 360,
			"height": 180
		},
		"EndScene": {
			"type": "script",
			"path": "script/scene/EndScene.js",
			"global": true
		},
		"GameScene": {
			"type": "script",
			"path": "script/scene/GameScene.js",
			"global": true
		},
		"TitleScene": {
			"type": "script",
			"path": "script/scene/TitleScene.js",
			"global": true
		}
	},
	"moduleMainScripts": {},
	"environment": {
		"sandbox-runtime": "3",
		"niconico": {
			"supportedModes": [
				"ranking"
			],
			"preferredSessionParameters": {
				"totalTimeLimit": 70
			}
		}
	},
	"globalScripts": []
}